    "string",
    "lion"
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.6",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "faucet": "0.0.1",
    "tape": "^4.5.1",
    "uglify-js": "^2.6.2"
  }
